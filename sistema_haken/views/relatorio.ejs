      					<div class = "portlet-header">
      					  <h3>
        						<i class = "fa fa-list"></i>
        						Lista de Salas
      					  </h3>
      					</div> <!-- /.portlet-header -->

      					<div class = "portlet-content">
    							<div class = "sala col-sm-5 col-md-3">
    							  <div class = "row-stat">

      								<a name = "a100" data-toggle = "modal" href = "#basicModal1" class = "ola ui-tooltip" data-placement = "top" title = "Veja os equipamentos desta sala">
      									   <li class = "fa fa-eye"></li>
      								</a>

      								<h3 class = "row-stat-value"> &nbsp; SALA - A100 &nbsp; </h3>

      								<a class = "oi nav-icon ui-tooltip" data-toggle = "tooltip" data-placement = "top" title = "Total de Equipamentos">
      								  <span class = " totalEquip label label-success row-stat-badge"> 0 </span>
      								</a>

    							  </div> <!-- /.row-stat -->
    							</div> <!-- /.col -->

    							<div class = "sala col-sm-5 col-md-3">
    							  <div class = "row-stat">

      								<a name = "G105" data-toggle = "modal" href = "#basicModal2" class = "ola ui-tooltip" data-placement = "top" title = "Veja os equipamentos desta sala">
